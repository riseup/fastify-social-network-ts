{
  "name": "social-network-api",
  "description": "Social network API",
  "version": "0.3.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=20.12.2"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "dev": "ts-node-dev src/index.ts --respawn",
    "test": "jest",
    "coverage": "jest --coverage",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "openapi": "ts-node static/openapi.ts",
    "db:generate": "npm run typeorm migration:generate src/migration/initial -- -d src/entity/data-source.ts",
    "db:migration": "npm run typeorm migration:run -- -d src/entity/data-source.ts",
    "db:seed": "ts-node src/seed/seed.ts",
    "db:up": "docker-compose --env-file .env -f docker/docker-compose.yml up -d",
    "db:down": "docker-compose -f docker/docker-compose.yml down && docker volume rm docker_postgres_data",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@fastify/cookie": "^9.4.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.15.0",
    "bcrypt": "^5.1.1",
    "class-validator": "^0.14.1",
    "fastify": "^4.28.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/bcrypt": "^5.0.2",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0"
  }
}
